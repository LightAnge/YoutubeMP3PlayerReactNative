{
  "expo": {
    "name": "Y.L.C.",
    "slug": "youtubelightclient",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/icons/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "y"
    },
    "android": {
      "package": "com.chupin.pibeat",
      "adaptiveIcon": {
        "foregroundImage": "./images/icons/icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./images/icons/icon.png"
    },
    "description": ""
  }
}
